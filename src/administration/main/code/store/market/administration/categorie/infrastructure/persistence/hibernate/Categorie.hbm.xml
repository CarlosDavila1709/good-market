<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="store.market.administration.categorie.domain.Categorie" table="categories">
        <composite-id name="id" class="store.market.administration.categorie.domain.CategorieId" access="field">
            <key-property column="id" name="value" length="36" access="field" />
        </composite-id>
        <component name="groceryId" class="store.market.administration.categorie.domain.BackofficeGroceryId" access="field">
            <property name="value" column="grocery_id" type="string" access="field" />
        </component>
        <component name="name" class="store.market.administration.categorie.domain.CategorieName" access="field">
            <property name="value" column="name" type="string" access="field" />
        </component>
    </class>
</hibernate-mapping>